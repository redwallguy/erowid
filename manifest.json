{
	"manifest_version": 2,

	"name": "Erowid",
	"description": "This extension links applicable chemical names to the corresponding Erowid page",
	"version": "1.3",
	"icons": {
		"128": "ErowidIcon.png"
	},

	"browser_action": {
  	"default_icon": "ErowidIcon.png"
  	},

  	"permissions": ["https://www.erowid.org/general/big_chart.shtml", 
	"webNavigation", "tabs", "<all_urls>", "storage"],

	"background": {
	"scripts": ["background.js", "jquery-1.12.2.min.js"],
	"persistent": false
	}
}